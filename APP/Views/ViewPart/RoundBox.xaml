<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="APP.Views.RoundBox">
    <Grid Margin="10" HorizontalOptions="FillAndExpand"
          VerticalOptions="FillAndExpand">
        <Rectangle x:Name="rect0" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" RadiusX="20" RadiusY="20"></Rectangle>
        <Rectangle x:Name="rect1" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" RadiusX="20" RadiusY="20"></Rectangle>
        <Rectangle x:Name="rect2" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" RadiusX="20" RadiusY="20"></Rectangle>
        <Rectangle x:Name="rect3" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand" RadiusX="20" RadiusY="20"></Rectangle>
    </Grid>
</ContentView>
